import turtle
turtle = turtle.Turtle()
turtle.possition()

turtle.home()
turtle.clear()
for i in range(3):
turtle.forward(200)
turtle.left(120)

turtle.home()
turtle.clear()
turtle.forward(200)
turtle.left(120)
turtle.forward(200)
turtle.left(120)
turtle.forward(200)

turtle.home()
turtle.clear()
for i in range(5):
turtle.forward(200)
turtle.left(144)